{
  "name": "nestjs-nextjs",
  "version": "1.0.0",
  "description": "Nestjs running nextjs",
  "main": "index.js",
  "repository": "https://github.com/viggojonasson/nestjs-nextjs.git",
  "author": "Viggo Jonasson",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd web && next",
    "build": "NODE_ENV=production nest build && cd web && next build && mv ./web/.next ./",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/serve-static": "^2.2.2",
    "next": "^12.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.7",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
